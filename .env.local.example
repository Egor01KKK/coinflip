# Coinbase Developer Platform (CDP) API Key
# Get this from: https://portal.cdp.coinbase.com/
NEXT_PUBLIC_CDP_API_KEY=your_cdp_api_key

# Coinbase Paymaster URL for gasless transactions
# Format: https://api.developer.coinbase.com/rpc/v1/base-sepolia/YOUR_KEY
NEXT_PUBLIC_PAYMASTER_URL=https://api.developer.coinbase.com/rpc/v1/base-sepolia/YOUR_KEY

# Deployed KingOfTheBase contract address on Base Sepolia
# This will be populated after contract deployment
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Private key for contract deployment (DO NOT COMMIT THIS FILE IF YOU FILL IT IN)
# Only needed for deploying contracts via Foundry
# Create a .env.local file (gitignored) with your actual keys
PRIVATE_KEY=your_deployer_private_key
